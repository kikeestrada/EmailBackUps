p Old School Stack Column
table.contenttable(align='center', width='600', border='0', cellpadding='0', cellspacing='0')
	tr
		td.block(valign='top')
			table(border='0', cellpadding='0', cellspacing='0', width='290', align='left')
				tr
					td
						table.mobile(bgcolor='#ffffff', border='0', cellpadding='0', cellspacing='0', width='290')
							tr
								td(bgcolor='#666666')
									font(style='font-family: Arial, Helvetica, sans-serif; font-size: 14px; color:#ffffff') Left Colum
			table(border='0', cellpadding='0', cellspacing='0', width='290', align='right')
				tr
					td(valign='top', align='right')
						table.mobile(bgcolor='#ffffff', border='0', cellpadding='0', cellspacing='0', width='290')
							tr
								td(bgcolor='#99000000')
									font(style='font-family: Arial, Helvetica, sans-serif; font-size: 14px; color:#ffffff') Right Column
