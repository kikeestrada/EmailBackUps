// ****************Starts Right Column Stack on Top********************
p Right Column Stack on Top
table.contenttable(width='600', border='0', cellpadding='0', cellspacing='0', align='center')
	tr
		td(align='left')
			table.blocktable(width='290', align='right', border='0', cellspacing='0', cellpadding='0', bgcolor='#FF9900')
				tr
					td
						font.appleLinks(style='font-family: Arial, Helvetica, sans-serif; font-size: 14px;') RIGHT
			table.blocktable(width='290', align='left', border='0', cellspacing='0', cellpadding='0', bgcolor='#999900')
				tr
					td
						font.appleLinks(style='font-family: Arial, Helvetica, sans-serif; font-size: 14px;') LEFT
// ****************Ends Right Column Stack on Top********************
